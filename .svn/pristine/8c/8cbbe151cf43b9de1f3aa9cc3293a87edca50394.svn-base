package com.fable.dsp.common.dto.dataswitch;

/**
 * 解析JSON的任务传输类
 * @author Administrator
 *
 */
public class TransTaskJsonDto {
    private String id;
    private String name;
    private String description;
    private String synchroType;//同步类型
    private boolean isrebuildTrigger;//是否重建触发器
    private boolean isdeleteSourceData;//是否删除源
    private boolean hasMappingTable;
    private boolean hasMappingColumn;
    
    

	public String getSynchroType() {
		return synchroType;
	}

	public void setSynchroType(String synchroType) {
		this.synchroType = synchroType;
	}

	public String getId() {
        return id;
    }
    
    public void setId(String id) {
        this.id = id;
    }
    
    public String getName() {
        return name;
    }
    
    public void setName(String name) {
        this.name = name;
    }
    
    public String getDescription() {
        return description;
    }
    
    public void setDescription(String description) {
        this.description = description;
    }

	public boolean isIsrebuildTrigger() {
		return isrebuildTrigger;
	}

	public void setIsrebuildTrigger(boolean isrebuildTrigger) {
		this.isrebuildTrigger = isrebuildTrigger;
	}

	public boolean isIsdeleteSourceData() {
		return isdeleteSourceData;
	}

	public void setIsdeleteSourceData(boolean isdeleteSourceData) {
		this.isdeleteSourceData = isdeleteSourceData;
	}

	public boolean isHasMappingTable() {
		return hasMappingTable;
	}

	public void setHasMappingTable(boolean hasMappingTable) {
		this.hasMappingTable = hasMappingTable;
	}

	public boolean isHasMappingColumn() {
		return hasMappingColumn;
	}

	public void setHasMappingColumn(boolean hasMappingColumn) {
		this.hasMappingColumn = hasMappingColumn;
	}
    
}
